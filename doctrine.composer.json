{
    "name": "petrknap/doctrine",
    "type": "project",
    "license": "MIT",
    "homepage": "https://petrknap.github.io/docs/",
    "authors": [
        {
            "name": "Petr Knap",
            "email": "dev@petrknap.cz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/Doctrine/*"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "petrknap/doctrine-orm": "*"
    },
    "autoload": {
        "psr-4": {
            "PetrKnap\\Doctrine\\ORM\\": "packages/Doctrine/ORM/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PetrKnap\\Doctrine\\ORM\\Test\\": "packages/Doctrine/ORM/tests"
        }
    }
}
