{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ucetnictvi\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
